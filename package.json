{
  "name": "my-qwik-docs-starter",
  "description": "Basic start point to build a docs site with Qwik",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "coverage": "vitest run --coverage --config ./vitest.config.ts",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write ./src",
    "fmt.check": "prettier --check ./src",
    "lint": "eslint \"src/**/*.ts*\"",
    "prepare": "husky install",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "test": "vitest --config ./vitest.config.ts --run",
    "test.watch": "vitest --config ./vitest.config.ts",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.1.4",
    "@builder.io/qwik-city": "^1.1.4",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/eslint": "8.37.0",
    "@types/node": "^20.1.4",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "@typescript-eslint/utils": "^5.59.8",
    "@vitest/coverage-c8": "^0.31.2",
    "autoprefixer": "^10.4.14",
    "eslint": "8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qwik": "^1.1.4",
    "glob": "^10.2.7",
    "gray-matter": "^4.0.3",
    "husky": "^8.0.3",
    "postcss": "^8.4.24",
    "postcss-import": "^15.1.0",
    "prettier": "2.8.8",
    "rehype-pretty-code": "^0.9.6",
    "tailwindcss": "^3.3.2",
    "typescript": "5.0.4",
    "undici": "5.22.1",
    "vite": "4.3.5",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "^0.31.2"
  }
}
